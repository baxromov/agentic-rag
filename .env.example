# =============================================================================
# MyAgenticRAGFramework - Environment Configuration
# =============================================================================
# Copy this file to .env and fill in your values.

# -- LangSmith (Required for LangGraph) --
# Get your API key from: https://smith.langchain.com/settings
LANGSMITH_API_KEY=lsv2_pt_...

# -- LLM Provider: "claude", "openai", or "ollama" --
LLM_PROVIDER=ollama

# -- Claude (Anthropic) --
ANTHROPIC_API_KEY=sk-ant-...
CLAUDE_MODEL=claude-sonnet-4-20250514

# -- OpenAI --
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-4o

# -- Ollama (runs on host, not in Docker) --
OLLAMA_BASE_URL=http://host.docker.internal:11434
OLLAMA_MODEL=llama3.1

# -- MinIO (S3-compatible) --
MINIO_ENDPOINT=http://minio:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=documents

# -- Qdrant --
QDRANT_URL=http://qdrant:6333
QDRANT_COLLECTION=documents

# -- Embedding (via Ollama) --
EMBEDDING_MODEL=nomic-embed-text:latest
EMBEDDING_DIM=768

# -- Reranker --
RERANKER_MODEL=jinaai/jina-reranker-v2-base-multilingual

# -- Model Server (reranker only) --
MODEL_SERVER_URL=http://model-server:8080

# -- Chunking --
CHUNK_SIZE=1000
CHUNK_OVERLAP=200

# -- Retrieval --
RETRIEVAL_TOP_K=10
RETRIEVAL_PREFETCH_LIMIT=20
RERANK_TOP_K=5
RRF_K=60

# -- Redis (LangGraph pub/sub) --
REDIS_URL=redis://redis:6379

# -- PostgreSQL (LangGraph state persistence) --
POSTGRES_URL=postgresql://langgraph:langgraph@postgres:5432/langgraph

# -- LangGraph API Server --
LANGGRAPH_API_URL=http://langgraph-server:8000

# -- Agent --
MAX_RETRIES=3
